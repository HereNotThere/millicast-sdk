{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["src"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [
    "plugins/markdown",
    "node_modules/jsdoc-export-default-interop/dist/index"
  ],
  "opts": {
    "template": "node_modules/clean-jsdoc-theme",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true,
    "theme_opts": {
      "title": "Millicast Realtime Broadcast SDK",
      "theme": "light",
      "menu": [
        {
          "title": "GitHub",
          "link": "https://github.com/millicast/realtime-broadcast-sdk",
          "target": "_blank"
        }
      ]
    }
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
